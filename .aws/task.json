{
  "containerDefinitions": [
      {
          "name": "web",
          "image": "798614671151.dkr.ecr.eu-west-1.amazonaws.com/bad-bank:latest",
          "cpu": 0,
          "portMappings": [
              {
                  "name": "web-3001-tcp",
                  "containerPort": 3001,
                  "hostPort": 3001,
                  "protocol": "tcp",
                  "appProtocol": "http"
              }
          ],
          "essential": true,
          "secrets": [
            {
              "name": "PUBLISH_URL",
              "valueFrom": "arn:aws:secretsmanager:eu-west-1:798614671151:secret:prod/badbank/env-BcdZMG"
            },
            {
              "name": "MONGO_URI",
              "valueFrom": "arn:aws:secretsmanager:eu-west-1:798614671151:secret:prod/badbank/env-BcdZMG"
            },
            {
              "name": "JWT_SECRET",
              "valueFrom": "arn:aws:secretsmanager:eu-west-1:798614671151:secret:prod/badbank/env-BcdZMG"
            },
            {
              "name": "AMAZON_REGION",
              "valueFrom": "arn:aws:secretsmanager:eu-west-1:798614671151:secret:prod/badbank/env-BcdZMG"
            },
            {
              "name": "AMAZON_CREDENTIALS_KEY_ID",
              "valueFrom": "arn:aws:secretsmanager:eu-west-1:798614671151:secret:prod/badbank/env-BcdZMG"
            },
            {
              "name": "AMAZON_CREDENTIALS_ACCESS_KEY",
              "valueFrom": "arn:aws:secretsmanager:eu-west-1:798614671151:secret:prod/badbank/env-BcdZMG"
            },
            {
              "name": "AMAZON_COGNITO_USER_POOL_ID",
              "valueFrom": "arn:aws:secretsmanager:eu-west-1:798614671151:secret:prod/badbank/env-BcdZMG"
            },
            {
              "name": "AMAZON_COGNITO_CLIENT_ID",
              "valueFrom": "arn:aws:secretsmanager:eu-west-1:798614671151:secret:prod/badbank/env-BcdZMG"
            },
            {
              "name": "AMAZON_COGNITO_API_VERSION",
              "valueFrom": "arn:aws:secretsmanager:eu-west-1:798614671151:secret:prod/badbank/env-BcdZMG"
            }
          ],
          "mountPoints": [],
          "volumesFrom": [],
          "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                "awslogs-create-group": "true",
                "awslogs-group": "/ecs/bad-bank-task-def",
                "awslogs-region": "eu-west-1",
                "awslogs-stream-prefix": "ecs"
              }
          }
      }
  ],
  "family": "bad-bank-task-def-x86",
  "executionRoleArn": "arn:aws:iam::798614671151:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
  },
  "tags": []
}