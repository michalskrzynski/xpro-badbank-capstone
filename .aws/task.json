{
  "containerDefinitions": [
      {
          "name": "web",
          "image": "798614671151.dkr.ecr.eu-west-1.amazonaws.com/bad-bank:latest",
          "cpu": 0,
          "portMappings": [
              {
                  "name": "web-3001-tcp",
                  "containerPort": 3001,
                  "hostPort": 3001,
                  "protocol": "tcp",
                  "appProtocol": "http"
              }
          ],
          "essential": true,
          "environmentFiles": [
            {
                "value": "arn:aws:s3:::bad-bank-env/badbank.env",
                "type": "s3"
            }
            ],
          "mountPoints": [],
          "volumesFrom": [],
          "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                "awslogs-create-group": "true",
                "awslogs-group": "/ecs/bad-bank-task-def",
                "awslogs-region": "eu-west-1",
                "awslogs-stream-prefix": "ecs"
              }
          }
      }
  ],
  "family": "bad-bank-task-def-x86",
  "executionRoleArn": "arn:aws:iam::798614671151:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
  },
  "tags": []
}